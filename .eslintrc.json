{
  "extends": [
     "eslint:recommended",
     "plugin:react/recommended"
   ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser":true,
    "node":true
  },
  "rules":{
    "semi":2,
    "quotes":["error", "single"],
    "react/prop-types": 0
  }
}
